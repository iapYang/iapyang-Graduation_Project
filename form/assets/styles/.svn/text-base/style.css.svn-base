.top-container {
  position: relative;
  height: 451px;
  overflow: hidden;
  background-color: transparent;
}

.logo-bg {
  width: 1920px;
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -960px;
}

.logo {
  width: 380px;
  position: absolute;
  top: 150px;
  left: 50%;
  margin-left: -190px;
}

.txt {
  width: 250px;
  position: absolute;
  top: 250px;
  left: 50%;
  margin-left: -125px;
}

.container .right {
    font-size: 20px;
    cursor: pointer;
}

.content {
  width: 85%;
  height: 100%;
  background-color: #fff;
  margin: 100px auto 0;
}

.en-text {
  font-family: ENtxt;
  letter-spacing: 4px;
  margin-left: -3px;
}

/*.en-text2 {
  letter-spacing: 3px;
}*/

.content .row {
  margin-top: 10px;
}
.content .divider {
  height: 50px;
  background-color: #fff;
}
.content .section {
  width: 100%;
  /*color: white;*/
  /*background-color: #cd487c;*/
  font-size: 30px;
}

.selectReg {
  margin-top: 30px;
}
 

.content .zh-text {
  font-family: ZHtxt;
  letter-spacing: 4px;
  /*display: inline-block;
  vertical-align: middle;*/
}
.content input {
  height: 1.3rem;
}

input[type=text], input[type=password], input[type=email], input[type=url], input[type=time], input[type=date], input[type=datetime-local], input[type=tel], input[type=number], input[type=search], textarea.materialize-textarea {
  margin: 40px 0 15px 0;
}
.content textarea.materialize-textarea {
  min-height: 2rem;
}
.content textarea {
  height: 2rem;
}
.content .input-field label.active {
  transform: translateY(-50%);
}
.content .option1 [type="radio"] + label:after, .content .option1 [type="radio"] + label:before {
  width: 20px;
  height: 20px;
  top: 18px !important;
}
.content textarea.materialize-textarea {
  min-height: 1.3rem;
}
textarea.materialize-textarea {
  padding: 0;
  margin: 40px 0 0 0;
}
.skilllab {
  top: 2rem !important;
}
.content .option5 [type="radio"] + label:after, .content .option5 [type="radio"] + label:before {
  width: 20px;
  height: 20px;
  top: 18px !important;
}
.content .detail {
  margin-top: 15px;
}
.content [type="checkbox"] + label:after, .content [type="checkbox"] + label:before {
  top: 25px !important;
}
.content .td-first {
  padding-bottom: 0;
}
.content .td-first .row {
  margin-bottom: 0;
}
.content .td-second .s12 span {
  font-size: 30px;
}

@font-face {
  font-family: 'Material Icons';
  font-style: normal;
  font-weight: 400;
  src: local("Material Icons"), local("MaterialIcons-Regular"), url("../font/material-design-icons/Material-Icons.woff") format("woff");
}
.material-icons {
  font-family: 'Material Icons';
  font-weight: normal;
  font-style: normal;
  font-size: 24px;
  line-height: 1;
  letter-spacing: normal;
  text-transform: none;
  display: inline-block;
  white-space: nowrap;
  word-wrap: normal;
  direction: ltr;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
}
